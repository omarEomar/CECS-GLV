<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
    <!-- This part define the view handler to be able to use tiles in the application -->
    <application>
        <message-bundle>com.beshara.csc.gn.map.presentation.resources.map</message-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>mapped_Data_div</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.mappeddata.MappedDataBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <!--<managed-bean>
        <managed-bean-name>mapTreeDivBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.mappeddata.MapTreeDivBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>-->
    <managed-bean>
        <managed-bean-name>mappedDataBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.mappeddata.MappedDataBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>divBackBean</property-name>
            <property-class>com.beshara.csc.gn.map.presentation.backingbean.mappeddata.MappedDataBackBean</property-class>
            <value>#{mapped_Data_div}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>objectTypeListBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.objecttype.ObjectTypeListBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>societyListBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.society.SocietyListBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>societyAddBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.society.SocietyAddBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>searchMinistriesCustomHelperBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.society.SearchMinistriesCustomHelperBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <!-- ************************** start societies   by Abdelrhman 3-12-2013  *********************************-->
    <managed-bean>
        <managed-bean-name>societiesListBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.society.ListBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <!-- ************************** start Object Types by Abdelrhman 3-12-2013  *********************************-->
    <managed-bean>
        <managed-bean-name>objectTypesListBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.objecttype.ListBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>societyRelationAddBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.societyrelations.SocietyRelationAddBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>listBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.gn.map.presentation.backingbean.societyrelations.ListBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <!-- Reports Bean -->
    <managed-bean>
        <managed-bean-name>ReportsFrameBean</managed-bean-name>
        <managed-bean-class>com.beshara.reports.presentation.backingbean.ReportsIFrameBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>societyadd</from-outcome>
            <to-view-id>/module/jsps/society/societyaddpage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>societylist</from-outcome>
            <to-view-id>/module/jsps/society/list.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>socrelationaddPage</from-outcome>
            <to-view-id>/module/jsps/societyrelations/socrelationadd.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>socrelationEditPage</from-outcome>
            <to-view-id>/module/jsps/societyrelations/socrelationedit.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>socrelationlist</from-outcome>
            <to-view-id>/module/jsps/societyrelations/list.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>