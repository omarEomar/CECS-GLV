<faces-config xmlns="http://java.sun.com/JSF/Configuration">
<!-- ###################START################## Strat of Decisions Section ###################SHEKA################# -->
    <managed-bean>
        <managed-bean-name>decisionListBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.nl.reg.integration.presentation.backingbean.decision.DecisionListBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>decisionMaintainBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.nl.reg.integration.presentation.backingbean.decision.DecisionMaintainBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>decisionMainDataMaintainBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.nl.reg.integration.presentation.backingbean.decision.details.DecisionMainDataMaintain</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>masterEntityKey</property-name>
            <value>#{decisionMaintainBean.pageDTO.code}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>decisionEmployeesMaintainBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.nl.reg.integration.presentation.backingbean.decision.details.DecisionEmployeesMaintain</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>masterEntityKey</property-name>
            <value>#{decisionMaintainBean.pageDTO.code}</value>
        </managed-property>
        <managed-property>
            <property-name>currentDetails</property-name>
            <value>#{decisionMaintainBean.pageDTO.empDecisionsDTOList}</value>
        </managed-property>
        <managed-property>
            <property-name>cancelDecisionMode</property-name>
            <value>#{decisionMaintainBean.cancelDecisionMode}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>decisionReferencesMaintainBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.nl.reg.integration.presentation.backingbean.decision.details.DecisionReferencesMaintain</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>masterEntityKey</property-name>
            <value>#{decisionMaintainBean.pageDTO.code}</value>
        </managed-property>
        <managed-property>
            <property-name>currentDetails</property-name>
            <value>#{decisionMaintainBean.pageDTO.decisionRefrencesDTOList}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>decisionMaterialsMaintainBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.nl.reg.integration.presentation.backingbean.decision.details.DecisionMaterialsMaintain</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>masterEntityKey</property-name>
            <value>#{decisionMaintainBean.pageDTO.code}</value>
        </managed-property>
        <managed-property>
            <property-name>currentDetails</property-name>
            <value>#{decisionMaintainBean.pageDTO.regDecisionMaterialsDTOList}</value>
        </managed-property>
    </managed-bean>
	
	<managed-bean>
        <managed-bean-name>regulationModulesMaintainBean</managed-bean-name>
        <managed-bean-class>com.beshara.csc.nl.reg.integration.presentation.backingbean.regulation.details.RegulationModulesMaintain</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>masterEntityKey</property-name>
            <value>#{regulationMaintainBean.pageDTO.code}</value>
        </managed-property>
        <managed-property>
            <property-name>moduleRelations</property-name>
            <value>#{regulationMaintainBean.pageDTO.moduleRelationsDTOList}</value>
        </managed-property>
    </managed-bean>
	
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>decisionmaindata</from-outcome>
            <to-view-id>/integration/reg/jsps/decision/decisionmaindatamaintain.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>decisionreferencesmaintain</from-outcome>
            <to-view-id>/integration/reg/jsps/decision/decisionreferencesmaintain.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>decMaterialsMaintain</from-outcome>
            <to-view-id>/integration/reg/jsps/decision/materials/mainMaintain.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>decisionemployeesmaintain</from-outcome>
            <to-view-id>/integration/reg/jsps/decision/decisionemployeesmaintain.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
<!-- ###################END################### End of Decision Section ######################SHEKA################## -->

